#/etc/systemd/system/temp_receiver.service
[Unit]
Description=MQTT Temp Receiver Service
After=network.target

[Service]
ExecStart=/usr/bin/python3 /opt/temp/receiver.py
WorkingDirectory=/opt/temp/
StandardOutput=inherit
StandardError=inherit
Restart=always
#User=pi

[Install]
WantedBy=multi-user.target
